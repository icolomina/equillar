{% extends 'base.html.twig' %}

{% block title %}Create a contract{% endblock %}

{% block body %}

<div class="container-fluid px-4">
  <h1 class="mt-4">Create a contract</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item active">Create a contract to start receiving funds</li>
    </ol>
  
  <div class="row" >
    <div class="col-xl-6">
      <div class="card mb-4">
        <div class="card-header">
          <i class="fas fa-chart-area me-1"></i>
            Create contract data
        </div>
        <div class="card-body">
          <div {{ react_component('CreateContract') }} </div>
        </div>
      </div>
    </div>
  </div>
</div>

{% endblock %}